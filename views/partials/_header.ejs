<header>
<h1 style="font-size:60px;">Tiny App</h1>
<% if (!users) { %>
<form action="/login" method="POST">

     <button><a href="/login" class= "form-inline">Please Login</a></button>
    
    
</form>
<% } else { %>
<form action="/logout" method="POST">
    <button type="submit" class="btn btn-primary">Sign Out</button>
    <p>Logged in as <%= users.email %></p>
    </form>
<% }; %>
</header>